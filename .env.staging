APP_NAME=ManaableHR
APP_ENV=staging
APP_KEY=base64:${APP_KEY}
APP_DEBUG=false
APP_URL=https://staging.manaablehr.com

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=warning

DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_DATABASE=manaablehr_staging
DB_USERNAME=staging_user
DB_PASSWORD=secure_staging_password

BROADCAST_DRIVER=redis
CACHE_DRIVER=redis
FILESYSTEM_DISK=s3
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
SESSION_LIFETIME=120

REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

FORCE_SSL=true

# Other configurations remain the same as .env but with staging-specific values
# Add staging-specific S3 bucket and other credentials as needed
